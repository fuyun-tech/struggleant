{
  "name": "struggleant",
  "version": "1.6.0",
  "description": "Website of struggleant.com",
  "keywords": [
    "Magazine",
    "CMS",
    "AI CMS",
    "Wutong CMS",
    "Angular",
    "Angular SSR",
    "SSR",
    "Fuyun"
  ],
  "author": {
    "name": "Fuyun",
    "url": "https://www.struggleant.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/fuyun-tech/struggleant#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuyun-tech/struggleant.git"
  },
  "bugs": {
    "url": "https://github.com/fuyun-tech/struggleant/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gulp": "gulp build",
    "build:prod": "npm run build && npm run build:gulp",
    "build:zip": "bash ./scripts/zip.sh",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "bash ./scripts/deploy.sh",
    "deploy:local": "bash ./scripts/deploy-local.sh",
    "deploy:manual": "bash ./scripts/deploy-manual.sh",
    "cloc": "cloc --exclude-dir=.git,.idea,.vscode,node_modules,dist,test,package-lock.json,.angular,.github,.nx ."
  },
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/platform-server": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@angular/ssr": "^20.1.4",
    "@fuyun/feed": "^1.0.0",
    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "murmurhash": "^2.0.1",
    "ng-zorro-antd": "^20.1.0",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "^7.8.2",
    "sitemap": "^8.0.0",
    "tslib": "^2.8.1",
    "ua-parser-js": "^2.0.4",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@types/express": "^4.17.23",
    "@types/jasmine": "^5.1.8",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.17.0",
    "@types/ua-parser-js": "^0.7.39",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "jasmine-core": "^5.9.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "less": "^4.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
